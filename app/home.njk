{% set page_class = home %}
{% extends "./app/partials/template.njk" %}
{% block main %}
<!-- start header -->
<!-- {% include "./app/partials/_header.njk"%} -->
<!-- end header -->

<!-- start hero section -->
<section class="container-fluid hero-section section-top-bottom-padding " id="hero-section">
    <div class="container-xl hero-bg text-center">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-10 col-12">
                <h1 class="text-white">Grow your users. Smarter.</h1>
                <p class="text-white opacity-75">Powerful, self-serve product and growth analytics to help you convert,
                    engage, and retain
                    more users. Trusted
                    by over 4,000 startups.</p>
            </div>
        </div>
        <form class="row g-3 mt-4 justify-content-center mb-5">
            <div class="col-auto">
                <input type="email" class="form-control border-1 border-primary rounded-1 p-2 mobile-view-input"
                    placeholder="Enter your email">
            </div>
            <div class="col-auto d-lg-block d-none">
                <button type="submit" class="btn btn-primary mb-3 ps-3 pe-3">Get started</button>
            </div>
            <div class="col-lg-12 mt-0 text-start text-md-center">
                <span class="sm-text opacity-75 text-white ps-2">We care about your data in our <a
                        class="sm-text opacity-75 text-white text-decoration-underline link" href="">privacy
                        policy</a></span>
            </div>
            <div class="col-auto d-lg-none d-block">
                <button type="submit" class="btn btn-primary mb-3 mobile-btn">Get started</button>
            </div>
        </form>
        <div class="row justify-content-center position-relative">
            <img src="/dist/assets/images/png/home/img-1.png" alt="hero-img-1"
                class="img-fluid hero-img position-absolute d-none d-md-block">
            <img src="/dist/assets/images/png/home/img-4.png" alt="hero-img-1"
                class="img-fluid hero-img position-absolute d-block d-md-none">
        </div>
    </div>
</section>
<!-- end hero section -->

<!-- start working section -->
<section class="container-fluid work-section" id="work-section">
    <div class="container-fluid bg-secondary section-top-bottom-padding">
        <div class="row gy-5 justify-content-center">
            <div class="col-lg-3 col-md-6 col-12 text-center">
                <div class="title-icon mb-2">
                    <img src="/dist/assets/images/svg/home/featured-icon-1.svg" alt="icon-1">
                </div>
                <p class="work-title mb-1">Share team inboxes</p>
                <p class="text-primary">Whether you have a team of 2 or 200, our shared team inboxes keep everyone on
                    the same page and in the loop.</p>
            </div>
            <div class="col-lg-3 col-md-6 col-12 text-center">
                <div class="title-icon mb-2">
                    <img src="/dist/assets/images/svg/home/featured-icon-2.svg" alt="icon-1">
                </div>
                <p class="work-title mb-1">Deliver instant answers</p>
                <p class="text-primary">An all-in-one customer service platform that helps you balance everything your
                    customers need to be happy.</p>
            </div>
            <div class="col-lg-3 col-md-6 col-12 text-center">
                <div class="title-icon mb-2">
                    <img src="/dist/assets/images/svg/home/featured-icon-3.svg" alt="icon-1">
                </div>
                <p class="work-title mb-1">Manage your team with reports</p>
                <p class="text-primary">Measure what matters with Untitled’s easy-to-use reports. You can filter,
                    export, and drilldown on the data in a couple clicks.</p>
            </div>
        </div>
    </div>
</section>
<!-- end working section -->

